{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "quotes": [
            0,
            "single"
        ],
        "semi": [
            1,
            "always"
        ]
    }
}
